<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Login_Regis_Button_Style">
        <item name="android:layout_width">268dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/white_100</item>
    </style>

    <style name="Login_Regis_EditText_Style">
        <item name="android:gravity">center</item>
        <item name="android:theme">@style/MyEditText</item>
        <item name="android:textColorHint">@color/register_login_edit_hint</item>
    </style>

    <style name="BottomDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/dialog_bottom_up</item>
        <item name="android:windowExitAnimation">@anim/dialog_bottom_down</item>
    </style>

    <style name="TitleIcon">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:paddingStart">11dp</item>
        <item name="android:paddingEnd">11dp</item>
        <item name="android:paddingTop">11dp</item>
        <item name="android:paddingBottom">11dp</item>
        <item name="android:layout_marginTop">45dp</item>
    </style>

    <style name="TitleIcon_back">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:paddingStart">18dp</item>
        <item name="android:paddingEnd">18dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:paddingBottom">14dp</item>
        <item name="android:layout_marginTop">45dp</item>
    </style>
</resources>