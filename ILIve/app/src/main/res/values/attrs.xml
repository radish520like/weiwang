<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomMyInfoItemView">
        <attr name="showTitleIcon" format="boolean" />
        <attr name="title" format="string" />
        <attr name="value" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomFontTextView">
        <attr name="textFont" format="enum" >
            <enum name="bold" value="0"/>
            <enum name="demibold" value="1"/>
            <enum name="extraLight" value="2"/>
            <enum name="Heavy" value="3"/>
            <enum name="Light" value="4"/>
            <enum name="Medium" value="5"/>
            <enum name="Normal" value="6"/>
            <enum name="Regular" value="7"/>
            <enum name="Semibold" value="8"/>
            <enum name="Thin" value="9"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomFontEditText">
        <attr name="editFont" format="enum" >
            <enum name="bold" value="0"/>
            <enum name="demibold" value="1"/>
            <enum name="extraLight" value="2"/>
            <enum name="Heavy" value="3"/>
            <enum name="Light" value="4"/>
            <enum name="Medium" value="5"/>
            <enum name="Normal" value="6"/>
            <enum name="Regular" value="7"/>
            <enum name="Semibold" value="8"/>
            <enum name="Thin" value="9"/>
        </attr>
    </declare-styleable>

</resources>